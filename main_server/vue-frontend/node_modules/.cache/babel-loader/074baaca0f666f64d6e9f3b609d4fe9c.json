{"ast":null,"code":"import axios from 'axios';\nimport std_but from \"/src/components/std_but.vue\";\nexport default {\n  data() {\n    return {\n      box_in: '',\n      error: ''\n    };\n  },\n\n  components: {\n    std_but\n  },\n  methods: {\n    clicked() {\n      if (this.box_in == '') {\n        return;\n      }\n\n      this.submitBut();\n    },\n\n    async submitBut() {\n      console.log('submitted, button');\n      const request = {\n        type: 'MusicSelection',\n        arguments: this.box_in\n      };\n      console.log(\"COOKIE\");\n\n      if (window.$cookies.isKey(this.box_in)) {\n        window.$cookies.set(this.box_in, parseInt(this.$cookies.get(this.box_in)) + 1);\n      } else {\n        window.$cookies.set(this.box_in, 1);\n      }\n\n      console.log(\"submitting box items\");\n\n      if (this.isFree() == 0) {\n        this.showBar(\"Device is currently playing music, please try again later\", '');\n        return;\n      }\n\n      if (this.isFree == 2) {\n        this.showBar(\"Cannot reach device, please try again later\", '');\n        return;\n      }\n\n      await axios.post('/api/selection/', request).then(response => {\n        console.log(response);\n        this.showBar(\"Submission accepted. You will now be redirected home\", 'G');\n        setTimeout(this.redirectHome, 2500);\n      }).catch(error => {\n        console.log(error);\n        this.showBar(\"Server error. Please try again later.\", '');\n      });\n    },\n\n    isFree() {\n      console.log('getting data');\n      axios.get('/api/status').then(response => {\n        console.log(\"The data\");\n        console.log(response.data);\n\n        if (response.data == 'FREE') {\n          console.log(\"returning true\");\n          return 1;\n        } else {\n          return 0;\n        }\n      }).catch(error => {\n        console.log(error);\n        console.log(\"Returning false because server err\");\n        return 2;\n      });\n    },\n\n    showBar(input, color) {\n      this.error = input;\n      var x = document.getElementById(\"snackbar\" + color);\n      x.className = \"show\";\n      setTimeout(function () {\n        x.className = x.className.replace(\"show\", \"\");\n      }, 3000);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAgBE,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,OAAP,MAAoB,6BAApB;AAEA,eAAe;AACbC,MAAI,GAAG;AACL,WAAO;AACLC,YAAM,EAAE,EADH;AAELC,WAAK,EAAE;AAFF,KAAP;AAID,GANY;;AAObC,YAAU,EAAC;AACTJ;AADS,GAPE;AAUbK,SAAO,EAAC;AACNC,WAAO,GAAG;AACR,UAAG,KAAKJ,MAAL,IAAe,EAAlB,EAAqB;AACnB;AACF;;AAEA,WAAKK,SAAL;AACD,KAPK;;AAQN,UAAMA,SAAN,GAAiB;AACbC,aAAO,CAACC,GAAR,CAAY,mBAAZ;AACA,YAAMC,OAAM,GAAI;AACdC,YAAI,EAAE,gBADQ;AAEdC,iBAAS,EAAE,KAAKV;AAFF,OAAhB;AAKAM,aAAO,CAACC,GAAR,CAAY,QAAZ;;AACA,UAAGI,MAAM,CAACC,QAAP,CAAgBC,KAAhB,CAAsB,KAAKb,MAA3B,CAAH,EAAsC;AACpCW,cAAM,CAACC,QAAP,CAAgBE,GAAhB,CAAoB,KAAKd,MAAzB,EAAkCe,QAAQ,CAAC,KAAKH,QAAL,CAAcI,GAAd,CAAkB,KAAKhB,MAAvB,CAAD,CAAR,GAA2C,CAA7E;AACF,OAFA,MAEO;AACLW,cAAM,CAACC,QAAP,CAAgBE,GAAhB,CAAoB,KAAKd,MAAzB,EAAiC,CAAjC;AAED;;AAEDM,aAAO,CAACC,GAAR,CAAY,sBAAZ;;AACA,UAAG,KAAKU,MAAL,MAAiB,CAApB,EAAsB;AACpB,aAAKC,OAAL,CAAa,2DAAb,EAAyE,EAAzE;AACA;AACF;;AAEA,UAAI,KAAKD,MAAL,IAAe,CAAnB,EAAqB;AACnB,aAAKC,OAAL,CAAa,6CAAb,EAA2D,EAA3D;AACA;AACF;;AAGA,YAAMrB,KAAI,CACPsB,IADG,CACE,iBADF,EACqBX,OADrB,EAEHY,IAFG,CAEEC,QAAO,IAAI;AACff,eAAO,CAACC,GAAR,CAAYc,QAAZ;AACA,aAAKH,OAAL,CAAa,sDAAb,EAAqE,GAArE;AACAI,kBAAU,CAAC,KAAKC,YAAN,EAAoB,IAApB,CAAV;AACD,OANG,EAMDC,KANC,CAMKvB,KAAI,IAAK;AAChBK,eAAO,CAACC,GAAR,CAAYN,KAAZ;AACA,aAAKiB,OAAL,CAAa,uCAAb,EAAqD,EAArD;AACD,OATG,CAAN;AAUH,KA7CK;;AA8CND,UAAM,GAAE;AACFX,aAAO,CAACC,GAAR,CAAY,cAAZ;AACAV,WAAI,CACDmB,GADH,CACO,aADP,EAEGI,IAFH,CAEQC,QAAO,IAAI;AACff,eAAO,CAACC,GAAR,CAAY,UAAZ;AACAD,eAAO,CAACC,GAAR,CAAYc,QAAQ,CAACtB,IAArB;;AAEA,YAAGsB,QAAQ,CAACtB,IAAT,IAAiB,MAApB,EAA2B;AACzBO,iBAAO,CAACC,GAAR,CAAY,gBAAZ;AACA,iBAAO,CAAP;AACF,SAHA,MAGO;AACL,iBAAO,CAAP;AACF;AAED,OAbH,EAaKiB,KAbL,CAaWvB,KAAI,IAAK;AAChBK,eAAO,CAACC,GAAR,CAAYN,KAAZ;AACAK,eAAO,CAACC,GAAR,CAAY,oCAAZ;AACA,eAAO,CAAP;AACD,OAjBH;AAkBL,KAlEK;;AAmENW,WAAO,CAACO,KAAD,EAAQC,KAAR,EAAe;AACpB,WAAKzB,KAAL,GAAawB,KAAb;AACA,UAAIE,IAAIC,QAAQ,CAACC,cAAT,CAAwB,aAAaH,KAArC,CAAR;AACAC,OAAC,CAACG,SAAF,GAAc,MAAd;AACAR,gBAAU,CAAC,YAAU;AAAEK,SAAC,CAACG,SAAF,GAAcH,CAAC,CAACG,SAAF,CAAYC,OAAZ,CAAoB,MAApB,EAA4B,EAA5B,CAAd;AAAgD,OAA7D,EAA+D,IAA/D,CAAV;AACD;;AAxEK;AAVK,CAAf","names":["axios","std_but","data","box_in","error","components","methods","clicked","submitBut","console","log","request","type","arguments","window","$cookies","isKey","set","parseInt","get","isFree","showBar","post","then","response","setTimeout","redirectHome","catch","input","color","x","document","getElementById","className","replace"],"sourceRoot":"","sources":["/Users/18chanp1/Library/CloudStorage/OneDrive-UBC/Active Use/UBC/20 Year 2/16 CPEN 291/04 Projects/P2_A_G20/main_server/vue-frontend/src/components/drop_down.vue"],"sourcesContent":["<template>\n    <select class = \"Generaldd\" id=\"ddin\" v-model=\"box_in\">\n        <option class=\"Generaldd\" value=\"Mario\">Mario Theme Song</option>\n        <option class=\"Generaldd\" value=\"RickRoll\">Never gonna give you up</option>\n        <option class=\"Generaldd\" value=\"GoukurakuJoudo\">Zheng's Select: Goukuraku Joudo</option>\n    </select>\n    &nbsp; &nbsp; &nbsp;\n    <!-- <button @click = \"clicked()\">Submit</button> -->\n    <std_but type = \"button\" @pressed-button = \"clicked()\" text = \"Submit\" class=\"flexbut2\"/>\n\n    <div id=\"snackbar\">{{error}}</div>\n    <div id=\"snackbarG\">{{error}}</div>\n\n</template>\n\n<script>\n  import axios from 'axios'\n  import std_but from \"/src/components/std_but.vue\"\n\n  export default {\n    data() {\n      return {\n        box_in: '',\n        error: '',\n      }\n    },\n    components:{\n      std_but,\n    },\n    methods:{\n      clicked() {\n        if(this.box_in == ''){\n          return\n        }\n\n        this.submitBut()\n      }, \n      async submitBut(){\n          console.log('submitted, button')\n          const request = {\n            type: 'MusicSelection',\n            arguments: this.box_in,\n          }\n\n          console.log(\"COOKIE\")\n          if(window.$cookies.isKey(this.box_in)){\n            window.$cookies.set(this.box_in, (parseInt(this.$cookies.get(this.box_in)) + 1))\n          } else {\n            window.$cookies.set(this.box_in, 1)\n            \n           }\n          \n          console.log(\"submitting box items\")\n          if(this.isFree() == 0){\n            this.showBar(\"Device is currently playing music, please try again later\",'')\n            return\n          }\n\n          if (this.isFree == 2){\n            this.showBar(\"Cannot reach device, please try again later\",'')\n            return\n          }\n\n\n          await axios \n            .post('/api/selection/', request)\n            .then(response =>{\n              console.log(response)\n              this.showBar(\"Submission accepted. You will now be redirected home\", 'G')\n              setTimeout(this.redirectHome, 2500)\n            }).catch(error => {\n              console.log(error)\n              this.showBar(\"Server error. Please try again later.\",'')\n            })\n      },\n      isFree(){\n            console.log('getting data')     \n            axios \n              .get('/api/status')\n              .then(response =>{\n                console.log(\"The data\")\n                console.log(response.data)\n\n                if(response.data == 'FREE'){\n                  console.log(\"returning true\")\n                  return 1\n                } else {\n                  return 0\n                }\n              \n              }).catch(error => {\n                console.log(error)\n                console.log(\"Returning false because server err\")\n                return 2\n              })\n      },\n      showBar(input, color) {\n        this.error = input\n        var x = document.getElementById(\"snackbar\" + color);\n        x.className = \"show\";\n        setTimeout(function(){ x.className = x.className.replace(\"show\", \"\"); }, 3000);\n      },\n    }\n  }\n</script>\n\n<style>\n#ddin{\n  width: 100%;\n  margin-bottom: 15px;\n  padding: 6px 12px;\n\n  height: auto;\n  max-width: 100%;\n  min-height: 37px;\n\n  display: block;\n\n  font-size: 16px;\n  font-weight: 500;\n  line-height: 1.4375;\n  color: #555;\n  background-color: #fff;\n  background-image: none;\n  border: 1px solid #ccc;\n  border-radius: 4px;\n\n  box-shadow: inset 0 1px 1px rgba(0,0,0,.075);\n\n  transition: border-color ease-in-out .15s,box-shadow ease-in-out .15s,-webkit-box-shadow ease-in-out .15s;\n  \n\n  /* box-sizing: border-box; */\n}\n\n.Generaldd{\n    margin: auto;\n    max-width: 55ch;\n    text-align: left;\n    justify-content: left;\n    font-family: Noto Sans;\n    font-size: 16px;\n    font-weight: 400;\n    text-align: left;\n  }\n\n  .flexbut2{\n    font-family: Noto Sans;\n    background-color: #26374a;\n    color:white;\n    margin: 3px;\n    padding: 20px;\n    font-size: 16px;\n    text-align: center;\n    border-radius: 4px;\n    border: none;\n    /* min-width: 10ch; */\n    width: 100%;\n    height: 70px;\n  }\n\n  #snackbar {\n  border-radius: 4px;\n  visibility: hidden;\n  min-width: 250px;\n  margin-left: -125px;\n  background-color: #BB6464;\n  color: #fff;\n  text-align: center;\n  border-radius: 2px;\n  padding: 16px;\n  position: fixed;\n  z-index: 1;\n  left: 50%;\n  bottom: 30px;\n  font-size: 20px;\n}\n#snackbarG{\n  border-radius: 4px;\n  visibility: hidden;\n  min-width: 250px;\n  margin-left: -125px;\n  background-color: #88FF88;\n  color: #000000;\n  text-align: center;\n  border-radius: 2px;\n  padding: 16px;\n  position: fixed;\n  z-index: 1;\n  left: 50%;\n  bottom: 30px;\n  font-size: 20px;\n}\n#snackbar.show {\n  visibility: visible;\n  -webkit-animation: fadein 0.5s, fadeout 0.5s 2.5s;\n  animation: fadein 0.5s, fadeout 0.5s 2.5s;\n}\n#snackbarG.show {\n  visibility: visible;\n  -webkit-animation: fadein 0.5s, fadeout 0.5s 2.5s;\n  animation: fadein 0.5s, fadeout 0.5s 2.5s;\n}\n@-webkit-keyframes fadein {\n  from {bottom: 0; opacity: 0;} \n  to {bottom: 30px; opacity: 1;}\n}\n@keyframes fadein {\n  from {bottom: 0; opacity: 0;}\n  to {bottom: 30px; opacity: 1;}\n}\n@-webkit-keyframes fadeout {\n  from {bottom: 30px; opacity: 1;} \n  to {bottom: 0; opacity: 0;}\n}\n@keyframes fadeout {\n  from {bottom: 30px; opacity: 1;}\n  to {bottom: 0; opacity: 0;}\n}\n\ntextarea{\n    height : 15em;\n    width : 88vw;\n  }\n\n\n@media only screen and (min-width: 768px){\n  textarea{\n    height : 15em;\n    width : 75ch;\n  }\n}\n  \n</style>\n"]},"metadata":{},"sourceType":"module"}