{"ast":null,"code":"import axios from 'axios';\nimport std_but from '/src/components/std_but';\nexport default {\n  data() {\n    return {\n      box_in: ''\n    };\n  },\n\n  components() {\n    std_but;\n  },\n\n  methods: {\n    clicked() {\n      if (this.box_in == '') {\n        return;\n      }\n\n      this.submitBut();\n    },\n\n    async submitBut() {\n      console.log('submitted, button');\n      const request = {\n        type: 'MusicSelection',\n        arguments: this.box_in\n      };\n      console.log(\"submitting box items\");\n      await axios.post('/api/selection/', request).then(response => {\n        console.log(response);\n        this.showBar(\"Submission accepted. You will now be redirected home\", 'G');\n        setTimeout(this.redirectHome, 2500);\n      }).catch(error => {\n        console.log(error);\n        this.showBar(\"Server error. Please try again later.\", '');\n      });\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAgBE,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,OAAP,MAAoB,yBAApB;AAEA,eAAe;AACbC,MAAI,GAAG;AACL,WAAO;AACLC,YAAM,EAAE;AADH,KAAP;AAGD,GALY;;AAMbC,YAAU,GAAG;AACXH,WAAM;AACP,GARY;;AASbI,SAAO,EAAC;AACNC,WAAO,GAAG;AACR,UAAG,KAAKH,MAAL,IAAe,EAAlB,EAAqB;AACnB;AACF;;AAEA,WAAKI,SAAL;AACD,KAPK;;AAQN,UAAMA,SAAN,GAAiB;AACbC,aAAO,CAACC,GAAR,CAAY,mBAAZ;AACA,YAAMC,OAAM,GAAI;AACdC,YAAI,EAAE,gBADQ;AAEdC,iBAAS,EAAE,KAAKT;AAFF,OAAhB;AAKAK,aAAO,CAACC,GAAR,CAAY,sBAAZ;AAEA,YAAMT,KAAI,CACPa,IADG,CACE,iBADF,EACqBH,OADrB,EAEHI,IAFG,CAEEC,QAAO,IAAI;AACfP,eAAO,CAACC,GAAR,CAAYM,QAAZ;AACA,aAAKC,OAAL,CAAa,sDAAb,EAAqE,GAArE;AACAC,kBAAU,CAAC,KAAKC,YAAN,EAAoB,IAApB,CAAV;AACD,OANG,EAMDC,KANC,CAMKC,KAAI,IAAK;AAChBZ,eAAO,CAACC,GAAR,CAAYW,KAAZ;AACA,aAAKJ,OAAL,CAAa,uCAAb,EAAqD,EAArD;AACD,OATG,CAAN;AAUC;;AA3BC;AATK,CAAf","names":["axios","std_but","data","box_in","components","methods","clicked","submitBut","console","log","request","type","arguments","post","then","response","showBar","setTimeout","redirectHome","catch","error"],"sourceRoot":"","sources":["